public class Computadora {
    private double precio;
    private String marca;
    private int ram;

    public Computadora() {
    }

    public Computadora(double precio, String marca, int ram) {
        this.precio = precio;
        this.marca = marca;
        this.ram = ram;
    }

    public double getPrecio() {
        return precio;
    }

    public void setPrecio(double precio) {
        this.precio = precio;
    }

    public String getMarca() {
        return marca;
    }

    public void setMarca(String marca) {
        this.marca = marca;
    }

    public int getRam() {
        return ram;
    }

    public void setRam(int ram) {
        this.ram = ram;
    }

    @Override
    public String toString() {
        return "Computadora{" +
                "precio=" + precio +
                ", marca='" + marca + '\'' +
                ", ram=" + ram +
                '}';
    }
}
